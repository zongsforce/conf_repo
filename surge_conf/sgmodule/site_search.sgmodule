#!name=Site Search
#!desc=快捷搜索
#!category=tools

[URL Rewrite]
# ＞搜索

# bing: 必应
^https:\/\/www.google.com\/search\?q=bing\+([^&]+) https://bing.com/search?mkt=en-US&q=$1 302
# bd: 百度
^https:\/\/www.google.com\/search\?q=bd\+([^&]+) https://www.baidu.com/s?wd=$1 302
# ddg: DuckDuckGo 
^https:\/\/www.google.com\/search\?q=ddg\+([^&]+) https://duckduckgo.com/?ia=about&q=$1 302
# gg: Google 英文
^https:\/\/www.google.com\/search\?q=gm\+([^&]+) https://www.google.com/search?q=$1&hl=en&lr=lang_en&cr=countryUS&safe=off 302
# gm: Google 图片
^https:\/\/www.google.com\/search\?q=gm\+([^&]+) https://www.google.com/search?&tbm=isch&q=$1 302
# gh
^https:\/\/www.google.com\/search\?q=gh\+([^&]+) https://github.com/search?q=$1 302
# sof: Stack Overflow
^https:\/\/www.google.com\/search\?q=sof\+([^&]+) https://stackoverflow.com/search?q=$1 302
# se: StackExchange
^https:\/\/www.google.com\/search\?q=se\+([^&]+) https://stackexchange.com/search?q=$1 302
# wa: WolframAlpha
^https:\/\/www.google.com\/search\?q=wa\+([^&]+) https://www.wolframalpha.com/input/?i=$1 302
# wiki: 维基百科
^https:\/\/www.google.com\/search\?q=wiki(\+|%20)([^&]+) http://wikipedia.org/wiki/$2 302
# wk: 维基中文
^https:\/\/www.google.com\/search\?q=wk(\+|%20)([^&]+) https://zh.wikipedia.org/wiki/$2 302
# mg: Magi
^https:\/\/www.google.com\/search\?q=mg(\+|%20)([^&]+) https://magi.com/search?q=$2 302
# wz: 无追搜索
^https:\/\/www.google.com\/search\?q=wz\+([^&]+) https://www.wuzhuiso.com/s?q=$1 302
^https:\/\/www.google.com\/search\?q=([^+]+)\+wz https://www.wuzhuiso.com/s?q=$1 302
# yh: 油猴搜索
^https:\/\/www.google.com\/search\?q=yh\+([^&]+) https://greasyfork.org/zh-CN/scripts?q=$1 302
^https:\/\/www.google.com\/search\?q=([^+]+)\+yh https://greasyfork.org/zh-CN/scripts?q=$1  302
# ios: 苹果应用搜索
# ^https:\/\/www.google.com\/search\?q=ios\+([^&]+) https://www.qimai.cn/search/index/search/$1 302
# ^https:\/\/www.google.com\/search\?q=([^+]+)\+ios https://www.qimai.cn/search/index/search/$1 302


